const userData = `#!/bin/bash
set -e
sudo apt update && sudo apt upgrade -y
sudo apt-get install -y ca-certificates curl
curl -fsSL https://get.docker.com -o get-docker.sh
sudo sh get-docker.sh
sudo docker run -d -p 14022:22 -p 9000:9000 -p 14240:14240 --name tigergraph --ulimit nofile=1000000:1000000 -v ~/data:/home/tigergraph/mydata -v tg-data:/home/tigergraph -t tigergraph/tigergraph:latest
`;
